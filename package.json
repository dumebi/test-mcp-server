{
    "name": "test-mcp-server",
    "version": "0.1.0",
    "description": "A Model Context Protocol server example",
    "private": true,
    "type": "module",
    "bin": {
        "test-mcp-server": "./build/index.js"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "git": "func() { git add . && git commit -m \"$1\" && git push origin HEAD; }; func",
        "build": "tsc",
        "start:server": "node build/server/google.js",
        "start:client": "node build/client/index.js",
        "prepare": "npm run build",
        "watch": "tsc --watch",
        "inspector": "npx @modelcontextprotocol/inspector build/server/google.js"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.54.0",
        "@modelcontextprotocol/sdk": "^1.13.0",
        "@slack/events-api": "^3.0.1",
        "@slack/oauth": "^3.0.3",
        "@slack/web-api": "^7.9.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "axios": "^1.9.0",
        "chrono-node": "^2.8.3",
        "cors": "^2.8.5",
        "cross-fetch": "^4.1.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "express": "^5.1.0",
        "googleapis": "^148.0.0",
        "luxon": "^3.6.1",
        "twitter-api-v2": "^1.24.0",
        "uuid": "^11.1.0",
        "zod": "^3.25.48"
    },
    "devDependencies": {
        "@types/luxon": "^3.6.2",
        "@types/node": "^20.11.24",
        "typescript": "^5.3.3"
    }
}
